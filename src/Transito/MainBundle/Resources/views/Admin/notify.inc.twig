{% block custom_js_footer %}
    <script src="{{ asset('rapido/assets/js/ui-notifications.js') }}"></script>
{% endblock %}
{% block ready_js %}
        toastr.options = {
          "closeButton": true,
          "positionClass": "toast-top-right",
          "onclick": null,
          "showDuration": "1000",
          "hideDuration": "1000",
          "timeOut": "5000",
          "extendedTimeOut": "1000",
          "showEasing": "swing",
          "hideEasing": "linear",
          "showMethod": "fadeIn",
          "hideMethod": "fadeOut"
        }
        UINotifications.init();
{% endblock %}